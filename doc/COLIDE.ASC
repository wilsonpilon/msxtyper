10 'SIMULACAO DA INSTR. ON SPRITE GOSUB
20 COLOR15,1,1:DEFINTA-F:SCREEN2
30 'DEFINE OS SPRITES,AS CORES E OS NOMES DOS SPRITES
40 FOR F=0TO7:READ A:VPOKE(14336+F),A:VPOKE(14344+F),A:NEXT F
50 VPOKE 6915,8:VPOKE 6919,7:VPOKE6914,0:VPOKE6918,1
60 'DEFINE A FORMA DO MOVIMENTO,POSICAO     E VERIFICA A FLAG DE SOBREPOSICAO
70 FOR F=0TO255
80 Y1=96+90*SIN(F/64*3.13):Y2=96+90*COS(F/64*3.14)
90 VPOKE 6912,Y1:VPOKE 6913,F
100 VPOKE 6916,Y2:VPOKE 6917,F
110 PSET(F,Y1),8:PSET(F,Y2),7
120 A=VDP(8):B=A AND 32:IFB=32THEN BEEP:A=0:B=0
130 NEXT F
140 CLS:GOTO 70
150 RETURN
160 DATA 0,64,124,127,124,64,0,0
