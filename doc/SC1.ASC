10 COLOR 15,4,4:SCREEN 1
20 A$="***** EDITORA ALEPH *****"
30 FOR L=1 TO LEN(A$)
40 VPOKE L+6144,ASC(MID$(A$,L))
50 NEXT L
60 FOR L=0 TO 1000:NEXTL
